head(lang='en')
  meta(charset='utf-8')
  meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
  
  if useStaticVendors===true
    link(rel="stylesheet" href="/vendors/fontawesome5/css/all.min.css")
    link(rel="stylesheet" href="/vendors/bootstrap5/css/bootstrap.min.css")
  else
    // Font-Awesome CSS
    link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css")
    // Bootstrap CSS
    link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous")
    
  link(rel="preconnect" href="https://fonts.googleapis.com")
  link(rel="preconnect" href="https://fonts.gstatic.com" crossorigin)
  link(href="https://fonts.googleapis.com/css2?family=Quicksand:wght@300&family=Anonymous+Pro&family=Urbanist:wght@100&display=swap" rel="stylesheet")
  
  // Main CSS file
  link(rel="stylesheet" href="/css/style.css")
  
  link(rel="shortcut icon" href="/favicon.ico")
  
  // Main Js CSS
  script(src="/js/main.js" defer)
  
  title= title
  meta(name='description' content='CSS , JS Calculator')
  
